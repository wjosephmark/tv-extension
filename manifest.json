{
  "manifest_version": 3,
  "name": "TVE-IBKR",
  "description": "TradingView InteractiveBrokers Buy/Sell Integration",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
